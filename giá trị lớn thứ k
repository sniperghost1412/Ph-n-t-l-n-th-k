#include <iostream>
using namespace std;
void NhapMang(int a[], int n)
{
	for (int i = 0; i < n; i++)
	{
		cout << "a[ " << i << " ]= " << endl;
		cin >> a[i];
	}
}
void XuatMang(int a[], int n)
{
	for (int i = 0; i < n; i++)
	{
		cout << "a [ " << i << " ]=" << a[i];
		cout << endl;
	}
}
int xoa(int a[], int &n)
{
	for (int i = 0; i<n - 1; i++)
		for (int k = i + 1; k<n; k++)
			if (a[k] == a[i])
			{
				for (int j = k; j<n - 1; j++)
					a[j] = a[j + 1];
				n--;
				k--;
			}
	return n;
}
void SapXep(int a[], int n)
{
	int k;
	for (int i = 0; i < n - 1; i++) 
	{
		for (int j = i + 1; j < n; j++) 
		{
			if (a[i]<a[j]) 
				k = a[i];   
				a[i] = a[j];  
				a[j] = k;
		}
	}
}
int main()
{
	int n, a[100], k;
	cin >> n;
	NhapMang(a, n);
	XuatMang(a, n);
	xoa(a, n);
	SapXep(a, n);
	XuatMang(a, n);
	cout << "Gia tri lon thu may? " << endl;
	cin >> k;
	for (int i = 0; i < n; i++)
		if (i == k - 1)
			cout << "Gia tri lon thu" << k << " la " << a[i] << endl;
	system("pause");

}
